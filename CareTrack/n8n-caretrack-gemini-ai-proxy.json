      },
      "id": "webhook-trigger",
      "name": "Webhook",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1,
      "position": [250, 300],
      "webhookId": "caretrack-ai"
    },
    {
      "parameters": {
        "jsCode": "const crypto = require('crypto');\nconst queryType = $input.item.json.query_type;\nconst params = $input.item.json.params;\nconst queryString = JSON.stringify(params);\nconst hash = crypto.createHash('md5').update(queryString).digest('hex');\n\nreturn {\n  query_type: queryType,\n  params: params,\n  query_hash: hash,\n  user_message: params.medication_name || params.symptoms || params.medications || ''\n};"
      },
      "id": "create-hash",
      "name": "Create Hash",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [450, 300]
    },
    {
      "parameters": {
