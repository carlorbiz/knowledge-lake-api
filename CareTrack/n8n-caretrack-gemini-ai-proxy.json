        "sendQuery": true,
        "queryParameters": {
          "parameters": [
            {
              "name": "query_hash",
              "value": "=eq.{{ $json.query_hash }}"
            },
            {
              "name": "expires_at",
              "value": "=gt.{{ $now.toISO() }}"
            }
          ]
        },
        "sendHeaders": true,
        "headerParameters": {
