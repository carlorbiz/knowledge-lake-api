{
  "name": "Gamma API",
  "nodes": [
    {
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 1,
      "properties": {
        "authentication": "predefinedCredentialType",
        "predefinedCredentialType": "headerAuth",
        "headerAuth": {
          "name": "X-API-KEY",
          "value": "={{ $credentials.apiKey }}"
        }
      }
    }
  ],
  "credentials": [
    {
      "name": "gammaApi",
      "type": "n8n-credentials.headerAuth",
      "properties": [
        {
          "displayName": "API Key",
          "name": "apiKey",
          "type": "string",
          "default": ""
        }
      ]
    }
  ]
}
